import card.Card;

public class test {

	public static void main(String[] args) {
		Card fire_ball = new Card("fire_ball", "fire", 1, 1, 3);
		fire_ball.addbuff(0, "fire", 1, 1);
		fire_ball.addbuff(1, "uncool", 1, 1);
		
		System.out.println(fire_ball.buff[0]);
		System.out.println(fire_ball.buff[1]);
	}

}

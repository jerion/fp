package witch;

import charactor.Charactor;

public class Witch {

	public static Charactor witch(int level) {
		Charactor witch[] = new Charactor[4];
		
		witch[0] = new Charactor(1, 20, 1, 2);
		witch[1] = new Charactor(2, 25, 2, 5);
		witch[2] = new Charactor(3, 30, 2, 10);
		witch[3] = new Charactor(4, 35, 2, 17);
		
		return witch[level];
	}

}
